<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.RoleHasUsersEntity" table="role_has_users" schema="beautysalon">
        <composite-id mapped="true" class="Entities.RoleHasUsersEntityPK">
            <key-property name="roleName">
                <column name="role_name" sql-type="varchar(15)" length="15"/>
            </key-property>
            <key-property name="usersIdUser">
                <column name="users_id_user" sql-type="int"/>
            </key-property>
        </composite-id>
        <many-to-one name="roleByRoleName" class="Entities.RoleEntity">
            <column name="role_name"/>
        </many-to-one>
    </class>
</hibernate-mapping>